{
  "name": "OpenVPN Client",
  "version": "1.0",
  "slug": "openvpn_client",
  "description": "OpenVPN Client",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "startup": "services",
  "boot": "auto",
  "auto_update": true,
  "options": {
    "remote": "ovpn1-clapiers.osmoziswifi.com",
    "port": 10111,
    "protocol": "udp",
    "dev": "tap",
    "route_delay": 10,
    "resolv_retry": "infinite",
    "nobind": true,
    "persist_key": true,
    "persist_tun": true,
    "verb": 2,
    "comp_lzo": true,
    "ca_crt": "",
    "client_crt": "",
    "client_key": "",
    "remote_cert_tls": "server",
    "auth": "SHA512",
    "tls_cipher": "TLS-DHE-RSA-WITH-AES-256-GCM-SHA384:TLS-DHE-RSA-WITH-AES-128-GCM-SHA256:TLS-DHE-RSA-WITH-AES-256-CBC-SHA:TLS-DHE-RSA-WITH-CAMELLIA-256-CBC-SHA:TLS-DHE-RSA-WITH-AES-128-CBC-SHA:TLS-DHE-RSA-WITH-CAMELLIA-128-CBC-SHA",
    "username": "",
    "password": ""
  },
  "schema": {
    "remote": "str",
    "port": "int",
    "protocol": "str",
    "dev": "str",
    "route_delay": "int?",
    "resolv_retry": "str?",
    "nobind": "bool?",
    "persist_key": "bool?",
    "persist_tun": "bool?",
    "verb": "int?",
    "comp_lzo": "bool?",
    "ca_crt": "str?",
    "client_crt": "str?",
    "client_key": "str?",
    "remote_cert_tls": "str?",
    "auth": "str?",
    "tls_cipher": "str?",
    "username": "str?",
    "password": "str?"
  },
  "ports": {},
  "map": ["config:rw"],
  "host_network": true
}
